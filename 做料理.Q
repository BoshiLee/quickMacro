[Script]
chief = "喵喵魔力 Ver 3.7 [雙子一]--Model-食"
arr = Array("Attachment:\salt.bmp", "Attachment:\pepper.bmp", "Attachment:\onion.bmp", "Attachment:\fin.bmp", "Attachment:\egg.bmp")

retryX = -1
retryY = - 1 
saltX = 1
saltY = 1
Hwnd = Plugin.Window.Find(0, chief)
sRect = Plugin.Window.GetWindowRect(Hwnd)
coordinates = Split(sRect, "|")
Call Plugin.Window.Active(Hwnd)

Function clickRetry(intX, intY)
If intX > 0 And intY > 0 Then 
   MoveTo intX + 5, intY + 5
   LeftClick 1
   MoveTo intX, intY - 40
   Delay (100)
Else 
    TracePrint "找不到重試"
End If
End Function

While saltX > 0
For i = 0 To UBOUND(arr) 
FindPic coordinates(0), coordinates(1), coordinates(2), coordinates(3), arr(i), 0.9, intX, intY
If intX > 0 And intY > 0 Then 
	MoveTo intX + 10, intY + 10
  	LeftDoubleClick 1
  	Delay (100)
Else 
    TracePrint "找不到材料"
    EndScript
End If
Next

FindPic coordinates(0), coordinates(1), coordinates(2), coordinates(3), "Attachment:\retry.bmp", 0.9, retryX, retryY
For 2
	Delay (200)
	Call clickRetry(retryX, retryY)
Next
FindPic coordinates(0), coordinates(1), coordinates(2), coordinates(3), arr(0), 0.9, saltX, saltY
Wend

